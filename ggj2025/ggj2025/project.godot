; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Pipe Dreams"
run/main_scene="res://UI/Menu/Menu.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://Assets/Sprites/bubble_icon.png"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/stretch/mode="viewport"
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg")

[global_group]

Generator=""
Player=""
Level=""
pipes=""
walls=""
GameManager=""
Minimap=""

[network]

limits/debugger/max_errors_per_second=4000
limits/debugger/max_warnings_per_second=4000

[rendering]

textures/vram_compression/import_etc2_astc=true
